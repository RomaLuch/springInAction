<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="duke" class="Juggler">
        <constructor-arg value="15"/>
    </bean>

    <bean id="sonet29" class="Sonet29"/>

    <bean id="poeticDuke" class="PoeticJuggler">
        <constructor-arg value="15"/>
        <constructor-arg ref="sonet29"/>
    </bean>

    <bean id="saxaphone" class="Saxaphone"/>

    <bean id="piano" class="Piano"/>

    <bean id="keny" class="Instrumentalist">
        <property name="song" value="Let it Be"/>
        <property name="instrument" ref="piano"/>
    </bean>


</beans>